{
    "version": 2,
    "builds": [
        {
            "src": "PersonalPage/PersonalPage.csproj",
            "use": "vercel-dotnet-builder@1.0.0"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "PersonalPage/PersonalPage.csproj"
        }
    ],
    "buildCommand": "dotnet publish -c Release"
} 